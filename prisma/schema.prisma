  id           Int @id @default(autoincrement())
  last_chat_id Int @default(0)

  @@schema("public")
}

model chat_embeddings {
  id            Int                    @id @default(autoincrement())
  chat_id       Int
  message       String
  created_at    DateTime?              @default(now()) @db.Timestamptz(6)
  role          String?
  embedding     Unsupported("vector")?
  // Relation to chat_messages removed due to type mismatch:
  // chat_embeddings.chat_id (Int) vs chat_messages.id (BigInt).
  // To restore, align DB types (make chat_embeddings.chat_id BigInt referencing chat_messages.id)
  // or reference a unique Int field on chat_messages. Until then, keep scalar and index.

  @@index([chat_id], map: "idx_chat_embeddings_chat_id")
  @@schema("public")
}

model chat_messages {
  id              BigInt                 @id @default(autoincrement())
  chat_id         Int
